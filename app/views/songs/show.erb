<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h2>"Successfully created song."</h2>
<%= @song.name %><br>
<a href='/artists/<%=@song.artist.slug %>'><%= @song.artist.name %></a><br>
<% @song.genres.each do |genre| %>
<a href='/genres/<%= genre.slug %>'><%= genre.name %></a>
<br>
<% end %>

